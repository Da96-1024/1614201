/* pages/login/login.wxss */
/**index.wxss**/

.container{
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 100rpx 0;
  box-sizing: border-box;
}

.classLogo{
  display: flex;
  flex-direction: row;
  align-items: center;
}

.classLogoText1{
  display: flex;
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  font-size: 78rpx;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.classLogoIcon{
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.classLogoText2{
  display: flex;
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  font-size: 78rpx;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.usermotto {
  margin-top: 300px;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}
.user-motto{
  font-size: 38rpx;
}

.motto_icon{
  width: 40rpx;
  height: 40rpx;
  margin-top: 10rpx;
  background-size: 100% 100%;
  background-image: url(data:image/svg+xml;base64,);
}

.login_btn{
 width: 360rpx;
  height: 88rpx;
  margin-top: 50rpx;
  display: table;
  background: #00ba07;
}

.click_style{
  background: #09bb47;
}

.login_text{
  font-size: 30rpx;
  color: #ffffff;
  display: table-cell;
  vertical-align: middle;
}
